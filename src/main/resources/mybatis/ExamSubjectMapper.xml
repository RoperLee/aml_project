<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.computer.boot.mapper.ExamSubjectMapper">

    <resultMap type="com.computer.boot.model.ExamSubject" id="ExamSubject">
        <result property="id" column="ID"/>
        <result property="gmt_creat" column="GMT_CREAT"/>
        <result property="is_deleted" column="IS_DELETED"/>
        <result property="full_name" column="FULL_NAME"/>
        <result property="short_name" column="SHORT_NAME"/>
        <result property="year" column="YEAR"/>
    </resultMap>

    <select id="getSubjectInfoById" resultMap="ExamSubject">
        SELECT * FROM EXAM_SUBJECT
        WHERE ID=#{id}
    </select>


</mapper>